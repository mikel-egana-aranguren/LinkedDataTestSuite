        <rdfs:label xml:lang="en">region</rdfs:label>
        <rdfs:label xml:lang="es">comunidad autónoma</rdfs:label>
        <rdfs:comment xml:lang="en">Property that specifies the region (comunidad autónoma) to which a municipality or any other region belongs</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://opendata.aragon.es/def/Aragopedia#gender -->


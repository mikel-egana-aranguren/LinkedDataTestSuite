    <!-- http://opendata.aragon.es/def/Aragopedia#contieneMunicipio -->

    <owl:ObjectProperty rdf:about="&aragodef;contieneMunicipio">
        <rdfs:label xml:lang="en">contains municipality</rdfs:label>
        <rdfs:label xml:lang="es">contiene municipio</rdfs:label>
        <rdfs:comment xml:lang="en">This property allows representing the municipalities that a Comarca, Province or Comunidad Autónoma contains</rdfs:comment>
        <rdfs:comment xml:lang="es">Esta propiedad permite determinar los municipios que una comarca, provincia o comunidad autónoma contiene</rdfs:comment>
        <rdfs:domain rdf:resource="&dbpedia;GovernmentalAdministrativeRegion"/>
        <rdfs:range rdf:resource="&dbpedia;Municipality"/>
    </owl:ObjectProperty>
    


    <!-- http://opendata.aragon.es/def/Aragopedia#enComarca -->

    <owl:ObjectProperty rdf:about="&aragodef;enComarca">
        <rdfs:label xml:lang="en">in comarca</rdfs:label>
        <rdfs:label xml:lang="es">en comarca</rdfs:label>
        <rdfs:comment xml:lang="en">This property identifies the comarca to which a municipality belongs</rdfs:comment>
        <rdfs:comment xml:lang="es">Esta propiedad identifica la comarca a la que pertenece un municipio</rdfs:comment>
        <rdfs:domain rdf:resource="&dbpedia;Municipality"/>
        <rdfs:range rdf:resource="&aragodef;Comarca"/>
    </owl:ObjectProperty>
    


    <!-- http://opendata.aragon.es/def/Aragopedia#enComunidadAutonoma -->

